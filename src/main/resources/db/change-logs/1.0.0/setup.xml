<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.26.xsd">
    <changeSet id="ifmo-hps-v1.0.0-start" author="${author}">
        <tagDatabase tag="ifmo-hps-v1.0.0"/>
    </changeSet>

    <changeSet id="ifmo-hps-v1.0.0-users" author="${author}">
        <sqlFile path="changes/01.users.sql" relativeToChangelogFile="true" dbms="postgresql"/>
        <rollback>
            <sqlFile path="changes/01.users.rollback.sql" relativeToChangelogFile="true"/>
        </rollback>
    </changeSet>

    <changeSet id="ifmo-hps-v1.0.0-end" author="${author}">
        <tagDatabase tag="ifmo-hps-v1.0.0-end"/>
    </changeSet>
</databaseChangeLog>